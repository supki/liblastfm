  default-language: Haskell2010
  build-depends:
      base             >= 4 && < 5
    , liblastfm        >= 0.4.0.0
    , text
    , lens             >= 4.4
    , aeson
  main-is: multitag-search.hs
  ghc-options:
    -Wall
    -fno-warn-unused-do-bind

